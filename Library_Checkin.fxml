<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="library_mgmt.Library_CheckinController">
   <children>
      <Text layoutX="408.0" layoutY="90.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Check-In">
         <font>
            <Font size="24.0" />
         </font>
      </Text>
      <Text fx:id="checkinText" layoutX="373.0" layoutY="305.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Item has been Checked in." visible="false">
         <font>
            <Font size="14.0" />
         </font>
      </Text>
      <Text fx:id="checkoutIDText" layoutX="190.0" layoutY="253.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Enter Checkout ID:">
         <font>
            <Font size="14.0" />
         </font>
      </Text>
      <TextField fx:id="checkoutIDField" layoutX="336.0" layoutY="234.0" prefHeight="26.0" prefWidth="303.0" />
      <Button fx:id="submitBtn" layoutX="336.0" layoutY="325.0" mnemonicParsing="false" onAction="#handleSubmitBtn" prefHeight="30.0" prefWidth="218.0" text="Submit">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="cancelBtn" layoutX="559.0" layoutY="325.0" mnemonicParsing="false" onAction="#handleCancelBtn" prefHeight="30.0" prefWidth="79.0" text="Cancel">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="returnBtn" layoutX="373.0" layoutY="386.0" mnemonicParsing="false" onAction="#handleReturnBtn" prefHeight="30.0" prefWidth="162.0" text="Return to Menu" visible="false">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Text fx:id="errText" fill="#f50202" layoutX="340.0" layoutY="406.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Checkout ID not found in the system." visible="false">
         <font>
            <Font size="14.0" />
         </font>
      </Text>
   </children>
</AnchorPane>
